{
  "name": "automation-best-practices",
  "version": "1.0.0",
  "description": "This is an Automation Best Practices workshop designed to teach testing through cross-functional automation",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --spec 'cypress/integration/07-ci/*'",
    "posttest": "npm run --prefix todomvc reset:db",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "npm start --prefix todomvc -- --quiet",
    "start:react": "http-server --silent -p 3000 -c-1 todomvc-react",
    "ci": "start-test 3000",
    "dev": "start-test 3000 cy:open",
    "postinstall": "cd todomvc && npm install",
    "reset": "npm run reset --prefix todomvc",
    "format": "eslint --fix 'cypress/**/*.js'",
    "lint": "eslint 'cypress/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saucelabs-training/automation-best-practices.git"
  },
  "keywords": [],
  "author": "Nikolay Advolodkin @nikolay_a00",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saucelabs-training/automation-best-practices/issues"
  },
  "homepage": "https://github.com/saucelabs-training/automation-best-practices#readme",
  
  "devDependencies": {
    "@babel/plugin-proposal-do-expressions": "7.5.0",
    "@cypress/browserify-preprocessor": "3.0.1",
    "@cypress/code-coverage": "1.14.0",
    "babel-plugin-istanbul": "5.2.0",
    "cypress": "6.5.0",
    "cypress-dark": "1.7.14",
    "cypress-failed-log": "2.9.1",
    "cypress-pipe": "1.7.0",
    "cypress-plugin-snapshots": "1.4.4",
    "cypress-react-unit-test": "2.10.0",
    "cypress-xpath": "1.6.2",
    "debug": "4.1.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-prettier": "3.1.2",
    "http-server": "0.12.0",
    "istanbul-lib-coverage": "3.0.0",
    "mocha": "6.2.2",
    "mocha-junit-reporter": "1.23.3",
    "mocha-multi-reporters": "1.1.7",
    "mochawesome": "4.1.0",
    "mochawesome-merge": "1.0.7",
    "mochawesome-report-generator": "4.0.1",
    "nyc": "15.0.0",
    "pluralize": "8.0.0",
    "prettier": "1.19.1",
    "start-server-and-test": "1.12.0",
    "tsify": "4.0.1",
    "typescript": "4.1.5"
  },
  "engines": {
    "node": ">=10"
  }
}
